<?xml version="1.0"?>
<configuration>
    <system.serviceModel>
        <bindings>
            <netTcpBinding>
                <binding name="NoSecurityTCP">
                    <security mode="None"/>
                </binding>
            </netTcpBinding>
            <basicHttpRelayBinding>
                <binding name="NoSecurityHTTP">
                    <security relayClientAuthenticationType="None"/>
                </binding>
            </basicHttpRelayBinding>
        </bindings>
        <client>
            <endpoint name="Service.MyService" address="net.tcp://localhost:9090/MyService" binding="netTcpBinding" bindingConfiguration="NoSecurityTCP" contract="*"/>
        </client>
        <behaviors>
            <serviceBehaviors>
                <behavior name="routingConfiguration">
                    <routing routeOnHeadersOnly="true" filterTableName="filterTable1"/>
                    <serviceDebug includeExceptionDetailInFaults="true"/>
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <routing>
            <filters>
                <filter name="MatchAllFilter" filterType="MatchAll"/>
            </filters>
            <filterTables>
                <filterTable name="filterTable1">
                    <add filterName="MatchAllFilter" endpointName="Service.MyService" priority="0"/>
                </filterTable>
            </filterTables>
        </routing>
        <services>
            <service name="System.ServiceModel.Routing.RoutingService" behaviorConfiguration="routingConfiguration">
                <endpoint address="MyService" binding="basicHttpRelayBinding" bindingConfiguration="NoSecurityHTTP" name="routerEndpoint1" contract="System.ServiceModel.Routing.IRequestReplyRouter"/>
            </service>
        </services>
    </system.serviceModel>

<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup></configuration>
